{
    "headers": [
        {
            "match": ".*\\.wasm",
            "headers": [
                {
                    "name": "content-type",
                    "value": "application/wasm"
                }
            ]
        },
        {
            "match": ".*",
            "headers": [
                {
                    "name": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "name": "Access-Control-Allow-Methods",
                    "value": "GET, OPTIONS"
                }
            ]
        }
    ]
}